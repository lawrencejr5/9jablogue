    {
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/**",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/*",
        "dest": "client/index.html"
      }
    ]
  }
  